<%= form_with(model: @event, url:'/select') do |form| %>
    <div class="checkbox">
        <%= collection_check_boxes(:event, :calendar_id, Calendar.all, :id, :title,include_hidden: false) do |b| %>
            <%= b.label class:"label-checkbox" do %>
                <%= b.check_box + b.text %>
            <% end %>
        <% end %>
    </div>
<% end %>