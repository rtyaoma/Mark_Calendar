<h1>Events#new</h1>
<p>Find me in app/views/events/new.html.erb</p>
<%= form_with url:"/events/create" do |form| %>
    <div class="field" >
      <%= form.label :title %>
      <%= form.text_field :title %>
    </div>
    <div class="field" >
        <%= form.label :start %>
        <%= form.datetime_select :start %>
    </div>
    <div class="field" >
        <%= form.label :end %>
        <%= form.datetime_select :end %>
    </div>
    <div class="field" >
      <%= form.label :place %>
      <%= form.text_area :place %>
    </div>
    <div class="field" >
      <%= form.label :description %>
      <%= form.text_area :description %>
    </div>
  
    <div class="action" >
      <%= form.submit value:"投稿" %>
    </div>
  
  <% end %>