<div class="main calendar-index">
  <h1>Calendars#index</h1>
  <p>Find me in app/views/calendars/index.html.erb</p>
  <%= form_with model:@calendars, url:'/select' do |form| %>
    <%= form.collection_select(:title,Calendar.all,:title,:title) %>
    <%= form.submit value="選択" %>
  <% end %>
  <div class="home">
  <% @calendars.each do |calendar| %>
    <%= link_to(calendar.title,"/calendars/#{calendar.id}") %>
  <% end %>
  </div>
</div>
